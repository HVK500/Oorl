{
  "manifest_version": 2,

  "name": "Oorl",
  "description": "This extension allows edit the various parameters of the current tabs URL",
  "icons": { 
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png" 
  },
  "version": "1.0",

  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "Oorl.html"
  }
}